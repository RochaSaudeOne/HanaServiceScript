zypper --non-interactive install  rpm-build
zypper --non-interactive install  xmlstarlet
zypper --non-interactive install  xorg-x11
zypper --non-interactive install  python2
zypper --non-interactive install  python-openssl
zypper --non-interactive install  libcap-progs
zypper --non-interactive install  samba
zypper --non-interactive install  samba-client
zypper --non-interactive install  libicu60_2
zypper --non-interactive install  openssl
zypper --non-interactive install  libopenssl-devel
zypper --non-interactive install  bc
zypper --non-interactive install  zip
zypper --non-interactive install  glibc-i18ndata
zypper --non-interactive install  nfs-kernel-server
zypper --non-interactive install  libatomic1
